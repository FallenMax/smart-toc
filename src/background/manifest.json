{
  "manifest_version": 3,
  "name": "Smart TOC / 智能网页大纲",
  "version": "0.7.0",
  "description": "Webpage outliner / 自动生成网页大纲、目录，Support Inoreader and Feedly。",
  "options_page": "options.html",
  "action": {
    "default_icon": "icon.png",
    "default_title": "Smart TOC / 智能网页大纲"
  },
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "toc.js"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy":{"extension_pages": "script-src 'self';object-src 'self';script-src-elem 'self' 'unsafe-inline' https://platform.twitter.com https://syndication.twitter.com;"},
  "commands": {
    "toggle": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E",
        "chromeos": "Ctrl+Shift+E",
        "linux": "Ctrl+Shift+E"
      },
      "description": "Load/unload TOC"
    },
    "prev": {
      "description": "Jump to previous heading"
    },
    "next": {
      "description": "Jump to next heading"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "contextMenus",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "author": "louchenabc@gmail.com"
}